CREATE TABLE noteEntity(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    content TEXT NOT NULL,
    colorHex INTEGER NOT NULL,
    created INTEGER NOT NULL
);

getAllNote:
SELECT *
FROM noteEntity;


getById:
SELECT *
FROM noteEntity
WHERE id = ?;

insertNote:
INSERT OR REPLACE
INTO noteEntity(
    id,
    title,
    content,
    colorHex,
    created
) VALUES (?, ?,?,?, ?);

deleteById:
DELETE FROM noteEntity
WHERE id = ?;
